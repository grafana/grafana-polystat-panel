    "@grafana/runtime": "8.4.11",
    "@grafana/ui": "8.4.11",
    "d3-hexbin": "0.2.2",
    "emotion": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-tooltip": "5.18.0",
    "tinycolor2": "^1.6.0",
    "tslib": "2.5.3"
  },
  "engines": {
    "node": ">= 16"
  },
  "packageManager": "yarn@1.22.19"
}
