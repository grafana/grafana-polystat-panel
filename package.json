{
  "name": "grafana-polystat-panel",
  "version": "2.0.9",
  "description": "Grafana Polystat Panel",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "test": "jest --watch --onlyChanged",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e-docker": "docker run --rm --add-host=host.docker.internal:host-gateway --volume $(pwd):/e2e --workdir /e2e --env='CYPRESS_HOST=host.docker.internal' --env='CYPRESS_BASE_URL=http://host.docker.internal:3000' --entrypoint '/bin/bash' cypress/included:9.5.1-node16.14.0-slim-chrome99-ff97 -c './node_modules/.bin/cypress install && yarn grafana-e2e run'",
    "e2e-docker:update": "docker run --rm --add-host=host.docker.internal:host-gateway --volume $(pwd):/e2e --workdir /e2e --env='CYPRESS_HOST=host.docker.internal' --env='CYPRESS_BASE_URL=http://host.docker.internal:3000' --entrypoint '/bin/bash' cypress/included:9.5.1-node16.14.0-slim-chrome99-ff97 -c './node_modules/.bin/cypress install && yarn grafana-e2e run --update-screenshots'",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:coverage": "jest --maxWorkers 4 --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grafana/grafana-polystat-panel.git"
  },
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/grafana/grafana-polystat-panel/issues",
    "email": "plugins@grafana.com"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/helper-validator-option": "7.18.6",
    "@grafana/e2e": "10.0.2",
    "@grafana/e2e-selectors": "10.0.2",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/d3": "7.4.0",
    "@types/d3-hexbin": "0.2.3",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.0",
    "@types/jquery": "3.5.14",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.10",
    "@types/tinycolor2": "1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cypress": "9.7.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.3.0",
